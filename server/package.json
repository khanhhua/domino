    "start": "node -r babel-register index.js",
    "debug": "node --inspect -r babel-register index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
